  },
  "homepage": "https://github.com/mcnultyshane/graphql_booksearch#readme",
  "dependencies": {
    "apollo-server-express": "^2.25.2",
    "dotenv": "^10.0.0",
    "if-env": "^1.0.4",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
